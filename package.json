{
  "name": "typexs-auth",
  "version": "0.0.2",
  "description": "Auth module for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts && karma start --single-run --browsers PhantomJS --code-coverage",
    "posttest": "nyc report --reporter=json && codecov -f coverage/**/*.json",
    "ng": "ng",
    "start": "concurrently \"ng serve -pc proxy.conf.json --open\" \"typexs server default\"",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mocha": "mocha --opts test/mocha.all.opts",
    "karma": "karma start --single-run --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-auth.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-auth/issues"
  },
  "homepage": "https://github.com/typexs/typexs-auth#readme",
  "devDependencies": {
    "@angular/cli": ">=1.7.4",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/deepmerge": "^1.3.3",
    "@types/gulp": "^4.0.5",
    "@types/gulp-watch": "^4.1.34",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^5.0.0",
    "@types/node": "~9.4.0",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.8",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "codelyzer": "~4.2.1",
    "del": "^3.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "gulpclass": "^0.1.2",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.3.3",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^3.0.12",
    "karma-typescript-preprocessor": "^0.3.1",
    "lodash": "^4.17.5",
    "mocha": "^5.1.0",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.6.0",
    "protractor": "~5.3.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "source-map": "^0.7.1",
    "sqlite3": "^4.0.0",
    "supertest": "^3.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.8.1",
    "typescript-require": "^0.2.10",
    "zone.js": "^0.8.20"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@ng-bootstrap/ng-bootstrap": "^1.1.2",
    "bcrypt": "^1.0.3",
    "bcrypt-nodejs": "0.0.3",
    "bootstrap": "^4.1.0",
    "class-validator": "^0.8.1",
    "core-js": "^2.5.5",
    "jsonwebtoken": "^8.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.10",
    "typexs-base": "latest",
    "typexs-ng": "latest",
    "typexs-server": "latest",
    "zone.js": "^0.8.26"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "report-dir": "./coverage/backend"
  },
  "typexs": {
    "name": "typexs-auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth",
          "src/adapters/auth"
        ]
      }
    ]
  }
}
