    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@types/passport": "^1.0.6",
    "@types/string-template": "^1.0.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "@typexs/base": "~1.3.3",
    "@typexs/ng": "~1.3.1",
    "@typexs/ng-base": "~1.3.1",
    "@typexs/roles": "~1.0.20",
    "@typexs/schema": "~1.1.0",
    "@typexs/server": "~1.1.0",
    "ajv": "^7.0.4",
    "animate.css": "^4.1.1",
