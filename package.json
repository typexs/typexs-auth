{
  "name": "typexs-auth",
  "version": "0.0.2",
  "description": "Auth module for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "ng": "ng",
    "start": "concurrently \"ng serve -pc proxy.conf.json --open\" \"typexs server default\"",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-auth.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-auth/issues"
  },
  "homepage": "https://github.com/typexs/typexs-auth#readme",
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/gulp-watch": "^4.1.34",
    "gulp-watch": "^5.0.0",
    "gulpclass": "^0.1.2",
    "lodash": "^4.17.5",
    "routing-controllers": "^0.7.6",
    "ts-node": "^5.0.0",
    "typexs-base": ">=0.3.17",
    "typexs-ng": "file:../packs/typexs-ng-0.0.2.tgz",
    "typexs-server": "file:../packs/typexs-server-0.1.6.tgz",
    "@angular/cli": ">=1.6.8",
    "@angular/compiler-cli": "^5.2.4",
    "@angular/language-service": "^5.2.4",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "~9.4.0",
    "gulp-typescript": "^4.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "~4.1.0",
    "karma": "~2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.0.0",
    "mocha-typescript": "^1.1.12",
    "protractor": "~5.3.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2",
    "typescript-require": "^0.2.9-1",
    "@angular/animations": "^5.2.4",
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/forms": "^5.2.4",
    "@angular/http": "^5.2.4",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "peerDependencies": {
    "typexs-base": ">=0.0.1",
    "typexs-server": ">=0.0.1",
    "typexs-ng": ">=0.0.2"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false
  },
  "typexs": {
    "name": "typexs-auth"
  }
