{
  "name": "@typexs/auth",
  "version": "1.0.0",
  "description": "Auth module for typexs",
  "main": "index.js",
  "directories": {
    "doc": "docs/de"
  },
  "scripts": {
    "test": "nyc mocha --opts test/mocha.all.opts && ng test typexs-auth --code-coverage=true --watch=false",
    "posttest": "nyc report --reporter=json && codecov -f coverage/backend/*.json && codecov -f coverage/karma/*.json",
    "ng": "ng",
    "start": "./node_modules/.bin/concurrently \"./node_modules/.bin/ng serve -pc proxy.conf.json --open\" \"./node_modules/.bin/typexs server --nodeId server\"",
    "build": "ng build --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "mocha": "mocha --opts test/mocha.all.opts",
    "karma": "ng test typexs-auth --code-coverage=true --watch=false",
    "build:lib": "ng-packagr -p bundles/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typexs/typexs-auth.git"
  },
  "author": "Robert Kania",
  "license": "MIT",
  "keywords": [
    "commons",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typexs/typexs-auth/issues"
  },
  "homepage": "https://github.com/typexs/typexs-auth#readme",
  "devDependencies": {
    "@allgemein/packaging": "^1.1.0",
    "@angular-devkit/core": "8.3.24",
    "@angular-devkit/schematics": "8.3.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@testdeck/mocha": "^0.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/gulp": "^4.0.6",
    "@types/gulp-watch": "^4.1.34",
    "@types/ldapjs": "^1.0.9",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.10.0",
    "@types/passport": "^1.0.4",
    "@types/string-template": "^1.0.2",
    "@types/superagent": "^4.1.10",
    "@types/supertest": "^2.0.10",
    "@typexs/base": "^1.2.31",
    "@typexs/ng": "^1.0.0",
    "@typexs/ng-base": "^1.0.0",
    "@typexs/roles": "^1.0.13",
    "@typexs/schema": "^1.0.13",
    "@typexs/server": "^1.0.11",
    "animate.css": "^4.1.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.7.2",
    "codelyzer": "6.0.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-shell": "^0.8.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "jquery": "^3.5.1",
    "karma": "~4.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^2.0.2",
    "ldapjs": "^1.0.2",
    "merge-stream": "^2.0.0",
    "mocha": "^7.0.1",
    "ng-packagr": "^5.7.1",
    "nyc": "^15.0.0",
    "passport": "^0.4.1",
    "passport-oauth": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "perfect-scrollbar": "^1.5.0",
    "pg": "^8.3.3",
    "pg-native": "^3.0.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.21.0",
    "protractor": "~7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "source-map": "^0.7.3",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "@typexs/roles-api": "^1.0.13",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "peerDependencies": {
    "@typexs/ng": "^1.0.0",
    "@typexs/schema": "^1.0.13",
    "@typexs/server": "^1.0.11",
    "@typexs/roles": "^1.0.13"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "json",
      "lcovonly"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "report-dir": "./coverage/backend"
  },
  "typexs": {
    "name": "typexs-auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth/*/*Adapter.*",
          "src/adapters/auth/*/*Adapter.*"
        ]
      },
      {
        "topic": "auth.configurations",
        "refs": [
          "adapters/auth/*/*Configuration.*",
          "src/adapters/auth/*/*Configuration.*"
        ]
      }
    ]
  }
}
