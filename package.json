    "@typexs/roles-api": "~1.0.19",
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1"
  },
  "peerDependencies": {
    "@typexs/base": "^1.3.3",
    "@typexs/ng": "^1.3.1",
    "@typexs/schema": "^1.1.0",
    "@typexs/server": "^1.1.0",
    "@typexs/roles": "^1.0.20"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "tmp/",
      "build/"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "json",
      "lcovonly"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "report-dir": "./coverage/backend"
  },
  "typexs": {
    "name": "typexs-auth",
    "declareLibs": [
      {
        "topic": "auth.adapters",
        "refs": [
          "adapters/auth/*/*Adapter.*",
          "src/adapters/auth/*/*Adapter.*"
        ]
      },
      {
        "topic": "auth.configurations",
        "refs": [
          "adapters/auth/*/*Configuration.*",
          "src/adapters/auth/*/*Configuration.*"
        ]
      }
    ]
  }
}
